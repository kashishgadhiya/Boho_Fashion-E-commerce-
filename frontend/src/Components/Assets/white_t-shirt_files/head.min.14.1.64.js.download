											var swiperid = $swiperParent
													.attr("id");
											if (!swiperid) {
												swiperid = "swi_"
														+ Math
																.floor(Math
																		.random() * 1E7);
												$swiperParent.attr("id",
														swiperid);
											}
											
											var isMobile = window.getTouchpoint()=='MOBILE' ? true : false;

											if (!pt.swipers[swiperid]) {
												pt.swipers[swiperid] = $swiperParent;
												$swiperParent
														.find(
																".swipe-pagination-button")
														.on('click',
